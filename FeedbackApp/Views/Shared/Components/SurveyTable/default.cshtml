@model List<FeedbackApp.Models.Survey>

<hr />

<div class="row pt-4">
    <div class="col">
        <table class="table">
            <thead>
                <tr>
                    <th>Survey</th>
                    <th>Description</th>
                    <th></th>
                    <th></th>
                    <th></th>


                </tr>
            </thead>
            <tbody>
                @for (var i = 0; i < Model.Count(); i++)
                {

                <tr>
                    <td>@Model[i].Title</td>
                    <td>@Model[i].Description</td>
                    <td><a asp-controller="Survey" asp-action="Index" asp-route-id="@Model[i].Id">Try survey</a></td>
                    <td><a asp-controller="CreateSurvey" asp-action="Index" asp-route-id="@Model[i].Id">Edit survey</a></td>
                    <td><a asp-controller="Home" asp-action="DeleteSurvey" asp-route-id="@Model[i].Id">Delete survey</a></td>

                </tr>

                }
            </tbody>
        </table>
    </div>
</div>
